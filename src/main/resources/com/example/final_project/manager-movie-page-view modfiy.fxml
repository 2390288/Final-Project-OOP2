<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="200.0" prefWidth="400.0" style="-fx-background-color: #EDF7F6;"
            xmlns="http://javafx.com/javafx/22"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.final_project.Controller.ManagerMovieModifyController">
    <children>
        <Label layoutX="160.0" layoutY="20.0" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="Modify Item" />
        <TextField fx:id="modifyTextField" layoutX="50.0" layoutY="70.0" prefWidth="300.0" />
        <Button fx:id="modifyButton" layoutX="150.0" layoutY="130.0" mnemonicParsing="false" style="-fx-background-color: #D3D3D3;" text="Modify" onAction="#handleModify" />
    </children>
</AnchorPane>
