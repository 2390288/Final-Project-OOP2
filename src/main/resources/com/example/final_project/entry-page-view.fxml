<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane style="-fx-background-color: #EDF7F6;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.final_project.Controller.EntryPageViewController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>

        <!-- Log In Button, anchored to the left and top -->
        <Button fx:id="logInButton" onAction="#onLogInButtonClick" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="129.0" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #0049B7; -fx-text-fill: white;" text="Log In" />

        <!-- Sign Up Button, anchored to the left and top -->
        <Button fx:id="signUpButton" onAction="#onSignUpButtonClick" AnchorPane.leftAnchor="160.0" AnchorPane.topAnchor="129.0" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: #FF1D58; -fx-text-fill: white;" text="Sign Up" />

        <!-- Entry Page Title Label, anchored to the left and top -->
        <Label fx:id="entryPageTitle" AnchorPane.leftAnchor="31.0" AnchorPane.topAnchor="40.0" prefHeight="30.0" prefWidth="259.0" style="-fx-font-size: 20px; -fx-font-weight: bold;" text="Welcome to the Entry Page" />
    </children>
</AnchorPane>
